{
  "Secrets": {
    "Provider": "aws_ssm_parameter",
    "ParameterStore": "/env/dev/authentication-service"
  },
  "Listen": [
    {
      "Protocol": "http",
      "Ip": "0.0.0.0",
      "Port": 80
    },
    {
      "Protocol": "https",
      "Ip": "0.0.0.0",
      "Port": 443,
      "Certificate": {
        "Path": "/web-certificate-data",
        "Password": "/web-certificate-password"
      }
    }
  ],
  "SigningCredential": {
    "Path": "/signing-credential-data",
    "Password": "/signing-credential-password"
  }
}
